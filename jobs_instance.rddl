non-fluents nf {
  domain = job_scheduler_domain;

  objects {
    job : {j1, j2, j3, j4, j5};
  };

  non-fluents {
    mu(j1) = 0.5;
    mu(j2) = 1;
    mu(j3) = 1;
    mu(j4) = 1;
    mu(j5) = 1;


    cost(j1) = -1.0;
    cost(j2) = -4.0;
    cost(j3) = 0;
    cost(j4) = 0;
    cost(j5) = 0;
  };
}

instance jobs_instance {
  domain = jobs_domain;
  non-fluents = nf;

  max-nondef-actions = 1;
  horizon = 10000;
  discount = 1.0;
}
